{
  "expo": {
    "name": "Janca",
    "slug": "Janca",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "Janca",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jsm.Janca"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/bemirs-font/Bemirs.ttf",
            "./assets/fonts/Cormorant_Garamond/Cormorant-Bold.ttf",
            "./assets/fonts/Cormorant_Garamond/Cormorant-Regular.ttf",
            "./assets/fonts/Cormorant_Garamond/Cormorant-Light.ttf",
            "./assets/fonts/Cormorant_Garamond/Cormorant-Medium.ttf",
            "./assets/fonts/Cormorant_Garamond/Cormorant-SemiBold.ttf",
            "./assets/fonts/Poppins/Poppins-Black.ttf",
            "./assets/fonts/Poppins/Poppins-Bold.ttf",
            "./assets/fonts/Poppins/Poppins-ExtraBold.ttf",
            "./assets/fonts/Poppins/Poppins-ExtraLight.ttf",
            "./assets/fonts/Poppins/Poppins-Light.ttf",
            "./assets/fonts/Poppins/Poppins-Medium.ttf",
            "./assets/fonts/Poppins/Poppins-Regular.ttf",
            "./assets/fonts/Poppins/Poppins-SemiBold.ttf",
            "./assets/fonts/Poppins/Poppins-Thin.ttf"
          ]
        }
      ]
    ]
  }
}
